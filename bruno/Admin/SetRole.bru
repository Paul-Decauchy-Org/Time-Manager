meta {
  name: SetRole
  type: graphql
  seq: 6
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  mutation SetRole($userID: ID!, $role: Role!){
    setRole(userID: $userID, role: $role){
      firstName
      lastName
      role
    }
  }
}

