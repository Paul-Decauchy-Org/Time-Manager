meta {
  name: mutation CreateMassiveUsers
  type: graphql
  seq: 1
}

post {
  url: http://localhost:8084/
  body: graphql
  auth: inherit
}

body:graphql {
  mutation CreateMassiveUsers($input: CreateMassiveUsersInput!) {
    createMassiveUsers(input: $input){
      id
      firstName
      lastName
      email
      phone
      password
      role
    }
  }
}

settings {
  encodeUrl: true
}
