meta {
  name: UpdateTeam
  type: graphql
  seq: 10
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  mutation UpdateTeam($id: ID!, $input: UpdateTeamInput!){
    updateTeam(id: $id, input: $input){
      name
      description
    }
  }
}

